{
  "extends": "airbnb",
  "rules": {
    "import/extensions":  0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  },
  "ecmaFeatures": {
    "modules": true,
    "spread" : true,
    "restParams" : true
  },
  "env" : {
    "browser" : true,
    "node" : true,
    "es6" : true
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
